{
	"name": "random-something",
	"displayName": "Random Something",
	"description": "随机生成一些内容，包括姓名，文字，身份证，图片，头像，手机号，邮箱等，方便开发者在布局的时候写页面",
  "version": "0.0.1",
  "publisher": "rufuswein",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:random-something.name",
        "onCommand:random-something.content"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
      "command": "random-something.name",
      "title": "Random: 姓名"
		}, {
      "command": "random-something.content",
      "title": "Random: 内容"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.47.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
		"mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }

}
